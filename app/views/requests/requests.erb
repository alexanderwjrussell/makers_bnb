<h1> Requests </h1>
<p></p>

<div id="booking-left">
  <h3> Requests I've made: </h3>

  <div id="">
    <ul id='bookings-requested'>
      <% @bookings_requested.each do |request| %>
        <li>
          <a <%= request.name %> ></a>
        </li>
        <li>
          <a <%= request.status %> ></a>
        </li>
        <li>
          <a <%= request.date %> ></a>
        </li>
      <% end %>
    </ul>
  </div>
</div>

<div id="booking-right">
  <h3> Requests I've received: </h3>

  <div id="">
    <ul id='bookings-received'>
      <% @bookings_received.each do |received| %>
        <li>
          <a <%= received.name %> ></a>
        </li>
        <li>
          <a <%= received.status %> ></a>
        </li>
        <li>
          <a <%= received.date %> ></a>
        </li>
      <% end %>
    </ul>
  </div>
</div>
